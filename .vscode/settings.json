{
    "python.pythonPath": "${workspaceFolder}\\.venv\\Scripts\\python.exe",
    "python.venvPath": ".venv",
    "python.linting.flake8Enabled": true,
    "python.linting.flake8Args": [
        "--max-line-length=127",
        "--max-complexity","20"
